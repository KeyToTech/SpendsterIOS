  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
    pod 'RxSwift',    '~> 4.0'
    pod 'RxCocoa',    '~> 4.0'
    # pod 'Alamofire',  '~> 5.0.0-beta.3'
    pod 'SwiftLint'
  # Pods for spendsterIOS

  target 'spendsterIOSTests' do
    inherit! :search_paths
    # Pods for testing
  end

end
